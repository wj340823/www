
package com.sucsoft.gzhbyjwz.bean.dc12;


public class AccidentSource {

    private String companyId;
    /**
     * 事故源
     * 
     */
    private String accidentSource;
    /**
     * 危险性类别
     * 
     */
    private String level;
    /**
     * 危险特性
     * 
     */
    private String hazardousCharacteristics;
    private String accidentsource_Id;
    /**
     * 泄漏应急措施
     * 
     */
    private String emergencyMeasure;

    public void setCompanyId(String companyId) {
        this.companyId = companyId;
    }

    public String getCompanyId() {
        return companyId;
    }

    public void setAccidentSource(String accidentSource) {
        this.accidentSource = accidentSource;
    }

    public String getAccidentSource() {
        return accidentSource;
    }

    public void setLevel(String level) {
        this.level = level;
    }

    public String getLevel() {
        return level;
    }

    public void setHazardousCharacteristics(String hazardousCharacteristics) {
        this.hazardousCharacteristics = hazardousCharacteristics;
    }

    public String getHazardousCharacteristics() {
        return hazardousCharacteristics;
    }

    public void setAccidentsource_Id(String accidentsource_Id) {
        this.accidentsource_Id = accidentsource_Id;
    }

    public String getAccidentsource_Id() {
        return accidentsource_Id;
    }

    public void setEmergencyMeasure(String emergencyMeasure) {
        this.emergencyMeasure = emergencyMeasure;
    }

    public String getEmergencyMeasure() {
        return emergencyMeasure;
    }

}
