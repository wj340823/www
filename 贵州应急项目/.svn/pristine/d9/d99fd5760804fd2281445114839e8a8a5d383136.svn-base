package com.sucsoft.gzhbyjwz.bean.dc12;

import io.swagger.annotations.ApiModel;
import io.swagger.annotations.ApiModelProperty;
import org.springframework.util.StringUtils;

@ApiModel(value = "联动单位", description = "联动单位")
public class ForeignAidUnit {
	@ApiModelProperty(value = "唯一标识")
	private String foreignAidUnitId;
	private String companyId;//所属企业id
	@ApiModelProperty(value = "单位部门")
	private String department;//部门
	@ApiModelProperty(value = "联系电话")
	private String telephone;//电话
	@ApiModelProperty(value = "职责")
	private String responsibility;
	public String getForeignAidUnitId() {
		return foreignAidUnitId;
	}
	public void setForeignAidUnitId(String foreignAidUnitId) {
		this.foreignAidUnitId = foreignAidUnitId;
	}
	public String getCompanyId() {
		return companyId;
	}
	public void setCompanyId(String companyId) {
		this.companyId = companyId;
	}
	public String getDepartment() {
		return department;
	}
	public void setDepartment(String department) {
		this.department = department;
	}
	public String getTelephone() {
		return telephone;
	}
	public void setTelephone(String telephone) {
		if (!StringUtils.isEmpty(telephone)){
			this.telephone = telephone.replaceAll(",", "");
		}
	}
	public String getResponsibility() {
		return responsibility;
	}
	public void setResponsibility(String responsibility) {
		this.responsibility = responsibility;
	}
	
}
