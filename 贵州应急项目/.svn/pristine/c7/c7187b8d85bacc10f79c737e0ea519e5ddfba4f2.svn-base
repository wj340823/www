<div  ng-controller="yilanbiao">
    <div class="clearfix">
        <div class="tb-title">突发环境事件情景分析-新增</div>
        <div class="fr">
            <button class="searchBtn" onclick="history.go(-1)">
                <i class="iconfont icon-fanhui"></i>
                返回
            </button>
        </div>
    </div>
    <div class="item-content" style="background-color: rgba(0,0,0,.7)">
        <div>
            <span  class="check-result1" style="width: 190px;">可能发生突发环境事件场所 ：</span>
            <textarea  class="text-info" ng-show="items.type!='edit'" ng-model="ylb.td10.place" ></textarea>
            <textarea  class="text-info" ng-show="items.type=='edit'" ng-model="ylb.qyscenario[items.index].place" ></textarea>
        </div>
        <br>
        <div>
            <span  class="check-result1" style="width: 190px;">应对措施：</span>
            <textarea  class="text-info" ng-show="items.type!='edit'" ng-model="ylb.td10.measures" ></textarea>
            <textarea  class="text-info" ng-show="items.type=='edit'" ng-model="ylb.qyscenario[items.index].measures" ></textarea>
        </div>
        <br>
        <div class="operate" >
            <button class="searchBtn" ng-click="items.type=='edit'?editYlb(10):postYlb(10)">保存</button>
            <div class="clearfix"></div>
        </div>
    </div>


    <div class="task-alert">
        <div class="alert-title">操作提示</div>
        <div class="alert-content">{{textContent}}</div>
        <ul class="alert-button">
            <li ng-click="sureAlert()" ng-if="textContent=='确定提交吗?'">确定</li>
            <li ng-click="resetAlert(textContent)">{{textContent=='确定提交吗?'?'取消':'关闭'}}</li>
        </ul>
    </div>
</div>
