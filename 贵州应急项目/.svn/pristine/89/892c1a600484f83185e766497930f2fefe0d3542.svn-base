package com.sucsoft.gzhbyjwz.bean.dc12;
/**
 * 风险物资说明
 * @author jinly
 *
 */
public class RiskMaterialExplain {

	private String riskMaterialExplainId;
	private String materiaName;//物资名称
	private String materiaType;//危险性类别
	private String materiaNature;//物化性质
	private String hazardCharacteristics;//危害特性
	public String getRiskMaterialExplainId() {
		return riskMaterialExplainId;
	}
	public void setRiskMaterialExplainId(String riskMaterialExplainId) {
		this.riskMaterialExplainId = riskMaterialExplainId;
	}
	public String getMateriaName() {
		return materiaName;
	}
	public void setMateriaName(String materiaName) {
		this.materiaName = materiaName;
	}
	public String getMateriaType() {
		return materiaType;
	}
	public void setMateriaType(String materiaType) {
		this.materiaType = materiaType;
	}
	public String getMateriaNature() {
		return materiaNature;
	}
	public void setMateriaNature(String materiaNature) {
		this.materiaNature = materiaNature;
	}
	public String getHazardCharacteristics() {
		return hazardCharacteristics;
	}
	public void setHazardCharacteristics(String hazardCharacteristics) {
		this.hazardCharacteristics = hazardCharacteristics;
	}
	
	
}
