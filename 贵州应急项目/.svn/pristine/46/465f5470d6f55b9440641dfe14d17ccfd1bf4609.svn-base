        "
        from
          EmergencyCrew
        where
          1=1
        #if($companyId)
          and companyId = :companyId
        #end
        #if($department)
          and DEPARTMENT = :department
        #end
        #if($keywords)
          and (name like :keywords or department like :keywords or theUtil like :keywords)
        #end
         order by department desc,theUtil desc
