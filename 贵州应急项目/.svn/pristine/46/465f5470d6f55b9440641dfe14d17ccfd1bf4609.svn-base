---
  grammer: HQL
  queries:
    #企业主要风险物资查询
    - name: getRiskMaterial
      resultType: RiskMaterial
      fetches: []
      statement:
        "
        from
          RiskMaterial
        where
          1=1
        #if($companyId)
          and companyId = :companyId
        #end
        "
