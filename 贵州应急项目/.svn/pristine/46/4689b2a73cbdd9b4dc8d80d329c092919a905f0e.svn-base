package com.sucsoft.gzhbyjwz.bean.param;

import com.sucsoft.gzhbyjwz.bean.dc12.WXHXP;
import com.sucsoft.gzhbyjwz.util.StringUtils;

import java.util.HashMap;
import java.util.Map;

public class WeixianParam implements Param{

    /**
     * 类型
     */
    private String HXPLX;

    /**
     * 中文名称
     */
    private String ZWMC;

    public String getHXPLX() {
        return HXPLX;
    }

    public void setHXPLX(String HXPLX) {
        this.HXPLX = HXPLX;
    }

    public String getZWMC() {
        return ZWMC;
    }

    public void setZWMC(String ZWMC) {
        this.ZWMC = ZWMC;
    }

    @Override
    public Map toParam() {
        Map param = new HashMap();
        if(this.HXPLX != null && !this.HXPLX.equals("")){
            param.put("HXPLX",this.getHXPLX());
        }
        if(this.ZWMC != null && !this.ZWMC.equals("")){
            param.put("ZWMC","%"+this.getZWMC()+"%");
        }
        return param;
    }
}
