<div  ng-controller="yilanbiao">
    <div class="clearfix">
        <div class="tb-title"> 应急物资-新增</div>
        <div class="fr">
            <button class="searchBtn" onclick="history.go(-1)">
                <i class="iconfont icon-fanhui"></i>
                返回
            </button>
        </div>
    </div>
    <div class="item-content" style="background-color: rgba(0,0,0,.7)">
        <div>
            <span class="check-result1">物资名称：</span>
            <input class="text-info-small" ng-show="items.type!='edit'" ng-model="ylb.td4.name" >
            <input class="text-info-small" ng-show="items.type=='edit'" ng-model="ylb.storages[items.index].name" >
            <br><br>
        </div>
        <br>
        <div>
            <span class="check-result1">数量：</span>
            <input class="text-info-small" ng-show="items.type!='edit'" ng-model="ylb.td4.number" >
            <input class="text-info-small" ng-show="items.type=='edit'" ng-model="ylb.storages[items.index].number" >
            <br><br>
        </div>
        <br>
        <div>
            <span class="check-result1">计量单位：</span>
            <input class="text-info-small" ng-show="items.type!='edit'" ng-model="ylb.td4.numberUtil" >
            <input class="text-info-small" ng-show="items.type=='edit'" ng-model="ylb.storages[items.index].numberUtil" >
            <br><br>
        </div>
        <br>
        <div>
            <span class="check-result1">规格/型号：</span>
            <input class="text-info-small" ng-show="items.type!='edit'" ng-model="ylb.td4.typeNumber" >
            <input class="text-info-small" ng-show="items.type=='edit'" ng-model="ylb.storages[items.index].typeNumber" >
            <br><br>
        </div>
        <br>
        <div>
            <span class="check-result1">贮存地点：</span>
            <input class="text-info-small" ng-show="items.type!='edit'" ng-model="ylb.td4.address" >
            <input class="text-info-small" ng-show="items.type=='edit'" ng-model="ylb.storages[items.index].address" >
            <br><br>
        </div>
        <br>
        <div class="operate" >
            <button class="searchBtn" ng-click="items.type=='edit'?editYlb(4):postYlb(4)">保存</button>
            <div class="clearfix"></div>
        </div>
    </div>

    <div class="task-alert">
        <div class="alert-title">操作提示</div>
        <div class="alert-content">{{textContent}}</div>
        <ul class="alert-button">
            <li ng-click="sureAlert()" ng-if="textContent=='确定提交吗?'">确定</li>
            <li ng-click="resetAlert(textContent)">{{textContent=='确定提交吗?'?'取消':'关闭'}}</li>
        </ul>
    </div>
</div>
