package com.sucsoft.gzhbyjwz.bean.dc12;

import io.swagger.annotations.ApiModel;
import io.swagger.annotations.ApiModelProperty;

@ApiModel(value = "应急专家", description = "应急专家")
public class EmergencySpecialist {

	@ApiModelProperty(value = "唯一标识")
	private String emergencySpecialistId;
	private String companyId;
	@ApiModelProperty(value = "姓名")
	private String name;
	@ApiModelProperty(value = "联系电话")
	private String telephone;
	@ApiModelProperty(value = "身份证号")
	private String sfzNum;
	@ApiModelProperty(value = "专业领域")
	private String professionalField;
	@ApiModelProperty(value = "性别")
	private String sex;
	@ApiModelProperty(value = "单位")
	private String theUtil;
	@ApiModelProperty(value = "部门")
	private String department;
	@ApiModelProperty(value = "职务")
	private String companyAgency;
	@ApiModelProperty(value = "单位地址")
	private String address;
		

	public String getEmergencySpecialistId() {
		return emergencySpecialistId;
	}
	public void setEmergencySpecialistId(String emergencySpecialistId) {
		this.emergencySpecialistId = emergencySpecialistId;
	}
	public String getCompanyId() {
		return companyId;
	}
	public void setCompanyId(String companyId) {
		this.companyId = companyId;
	}
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getTelephone() {
		return telephone;
	}
	public void setTelephone(String telephone) {
		this.telephone = telephone == null?null:telephone.replaceAll(",", "");
	}
	public String getSfzNum() {
		return sfzNum;
	}
	public void setSfzNum(String sfzNum) {
		this.sfzNum = sfzNum;
	}
	public String getProfessionalField() {
		return professionalField;
	}
	public void setProfessionalField(String professionalField) {
		this.professionalField = professionalField;
	}
	public String getSex() {
		return sex;
	}
	public void setSex(String sex) {
		this.sex = sex;
	}
	public String getTheUtil() {
		return theUtil;
	}
	public void setTheUtil(String theUtil) {
		this.theUtil = theUtil;
	}
	public String getDepartment() {
		return department;
	}
	public void setDepartment(String department) {
		this.department = department;
	}
	public String getCompanyAgency() {
		return companyAgency;
	}
	public void setCompanyAgency(String companyAgency) {
		this.companyAgency = companyAgency;
	}
	public String getAddress() {
		return address;
	}
	public void setAddress(String address) {
		this.address = address;
	}
	
}
