    <table style="margin-top: 10px" ng-show="search.emerStatus == '0'">
      <thead>
            <tr>
              <th>序号</th>
              <th>企业名称</th>
              <th>风险级别</th>
              <th>备案时间</th>
              <th>审核状态</th>
              <th>操作</th>
            </tr>
          </thead>
          <tbody>
            <tr ng-repeat="detail in page.unlist">
              <td ng-bind="$index+1">1</td>
              <td> {{ emergancy.company.companyName }} </td>
              <td> {{ detail.level=="3"?'重大风险源': detail.level=="2"?'较大风险源':detail.level=="1"?'一般风险源':''}} </td>
              <td> {{ detail.recordtime | date:'yyyy-MM-dd'}} </td>
              <td ng-class="detail.shzt==4?'dangerColor': detail.shzt==5?'dangerColor':detail.shzt==6?'dangerColor':detail.shzt==3?'finishColor':'warning'">
                  {{ detail.shzt==1?'区县已审核':detail.shzt==2?'市州已审核': detail.shzt==3?'已完结': detail.shzt==4?'区县驳回': detail.shzt==5?'市州驳回': detail.shzt==6?'省厅驳回':'待审核'}} </td>
                <td>
                    <span class="oper hander" ui-sref="enterpriseHead.addemergency({opType:'edit',index:$index,level:detail.level,id:detail.id})">修改</span>
                </td>
            </tr>
      </tbody>
    </table>

