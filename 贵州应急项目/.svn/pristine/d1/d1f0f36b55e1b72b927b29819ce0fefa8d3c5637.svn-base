package com.sucsoft.gzhbyjwz.bean.dc12;

import io.swagger.annotations.ApiModel;
import io.swagger.annotations.ApiModelProperty;

@ApiModel(value = "污染源处理设备/存储装置", description = "污染源处理设备")
public class ProcessEquipment {

	
	private String processEquipmentId;
	@ApiModelProperty(value = "种类")
	private String category;
	@ApiModelProperty(value = "最大容量")
	private String maxCapacity;
	@ApiModelProperty(value = "储存地点周边措施")
	private String peripheralMeasures;
	@ApiModelProperty(value = "分布情况")
	private String distribution;
	private String companyId;
	
	public String getCompanyId() {
		return companyId;
	}
	public void setCompanyId(String companyId) {
		this.companyId = companyId;
	}
	public String getProcessEquipmentId() {
		return processEquipmentId;
	}
	public void setProcessEquipmentId(String processEquipmentId) {
		this.processEquipmentId = processEquipmentId;
	}
	public String getCategory() {
		return category;
	}
	public void setCategory(String category) {
		this.category = category;
	}
	public String getMaxCapacity() {
		return maxCapacity;
	}
	public void setMaxCapacity(String maxCapacity) {
		this.maxCapacity = maxCapacity;
	}
	public String getPeripheralMeasures() {
		return peripheralMeasures;
	}
	public void setPeripheralMeasures(String peripheralMeasures) {
		this.peripheralMeasures = peripheralMeasures;
	}
	public String getDistribution() {
		return distribution;
	}
	public void setDistribution(String distribution) {
		this.distribution = distribution;
	}
}
