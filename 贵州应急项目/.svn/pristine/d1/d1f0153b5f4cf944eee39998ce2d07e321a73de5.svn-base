package com.sucsoft.gzhbyjwz.bean.constant;

public enum EmergencyTaskStatus {

    zhixingzhong("执行中",0),yujing("预警",1),chaoshi("超时",2),yiwancheng("已完成",10);

    private String name;
    private Integer status;

    //public static final long days = 7;
    public static final long daysInMillisecond = 7* 24 * 60 * 60 * 1000;

    EmergencyTaskStatus (String name,Integer status){
        this.name = name;
        this.status = status;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public Integer getStatus() {
        return status;
    }

    public void setStatus(Integer status) {
        this.status = status;
    }
}
