html body {
    margin: 0px;
    padding: 0px;
    font-family: "微软雅黑";
    background: url(../images/background.png) no-repeat fixed center;
    background-size: cover;
    height: 100%;
}

.login_body {
    width: 100%;
    height: 100%;
}

.login_main {
    position: absolute;
    left: -moz-calc(50% - 140px);
    left: -webkit-calc(50% - 140px);
    left: calc(50% - 140px);
    top: -moz-calc(50% - 110px);
    top: -webkit-calc(50% - 110px);
    top: calc(50% - 110px);
    text-align: center;
}

.login_main_left {
    height: 62px;
    width: 436px;
    background: url(../images/title.png) no-repeat;
    z-index: 1;
    margin-bottom: 45px;
}

.login_main_title {
    width: 100%;
    height: 90px;
    background: url(../images/title.png) no-repeat;
    background-position: center;
}

.login_main_right > h3 {
    height: 48px;
    color: #4389C5;
    font-size: 22px;
    padding-top: 12px;
    font-weight: 500;
    font-family: "微软雅黑";
}

.login_main_right {
    width: 284px;
    height: 198px;
    /* margin-left: 40px; */
    border-radius: 8px;
    box-shadow: 0 4px 10px rgba(0, 0, 0, 0.22);
}

.login_main_list {}

.leftInput {
    position: absolute;
    width: 35px;
    height: 20px;
    left: 15px;
    margin-top: 14px;
    border-right: 1px solid #393939;
}

.leftUser {
    background: url(../images/user.png) no-repeat;
}

.leftPsw {
    background: url(../images/passWord.png) no-repeat;
}

.login_main_list input {
    background-color: #EEEDED;
    /* 	border: 0px solid rgba(146, 151, 163,0.2);
	box-shadow: 1px 1px 1px rgba(0,0,0,0.35) inset; */
    /* for IE */
    /*filter:alpha(opacity=20);*/
    /* CSS3 standard */
    display: initial;
}

.login_main_list input[type='text'],
.login_main_list input[type='password'] {
    line-height: 35px;
    border-radius: 3px;
    color: #7b7b7b;
    padding-left: 60px;
    font-size: 16px;
    /* font-family: "AdobeHeitStd-Regular"; */
    font-family: "微软雅黑";
    width: 285px;
    height: 44px;
    background-color: rgba(255, 255, 255, 1);
    border: none;
}

.login_main_list li {
    padding: 0 0 20px 0;
    overflow: hidden;
}

.login_main_list input[type='check'] {
    width: 15px;
    height: 15px;
    border-radius: 2px;
    vertical-align: sub;
    padding: 2px;
    margin-right: 7px;
}

#checkCode {
    width: 176px;
    height: 40px;
    vertical-align: bottom;
}

#ok_id {
    background: #ffcc01;
    width: 300px;
    height: 44px;
    text-align: center;
    line-height: 34px;
    border-radius: 3px;
    display: inline-block;
    color: #fff;
    font-size: 18px;
    box-shadow: 1px 1px 1px rgba(0, 0, 0, 0) inset;
    cursor: pointer;
}

#ok_id.disabled,
#ok_id[disabled],
fieldset[disabled] #ok_id {
    cursor: not-allowed;
    filter: alpha(opacity=65);
    -webkit-box-shadow: none;
    box-shadow: none;
    opacity: .65;
}

.hide {
    display: none;
}

.checkFeedback {
    position: absolute;
    top: 10px;
    left: 200px;
    font-size: 20px;
    color: #408fe7;
}

.login_foot {
    position: absolute;
    bottom: 0px;
    width: 388px;
    height: 30px;
    overflow: hidden;
    background-color: rgba(236, 236, 232, 0.65);
    border-bottom-right-radius: 8px;
    border-bottom-left-radius: 8px
}

.login_foot > span {
    float: right;
    margin-right: 28px;
    padding-top: 5px;
    color: #4D4D4D;
}

.ok_btn:hover {
    text-decoration: none;
}

.elm {
    /*Firefox*/
    -moz-calc(expression);
    /*chrome safari*/
    -webkit-calc(expression);
    /*Standard */
    calc();
}

.errorMsg {
    font-size: 12px;
    color: red;
    line-height: 31px;
}

.login_title {
    width: 765px;
    height: 60px;
    position: absolute;
    top: 15%;
    left: 50%;
    transform: translate(-50%, 0);
    background-image: url(../images/title.png);
    background-repeat: no-repeat;
    background-position: center;
}

input:-webkit-autofill {
    -webkit-box-shadow: 0 0 0px 1000px rgba(255, 255, 255, 1) inset !important;
}

.bottom_title {
    position: absolute;
    left: 50%;
    bottom: 10px;
    transform: translate(-50%, 0);
}
