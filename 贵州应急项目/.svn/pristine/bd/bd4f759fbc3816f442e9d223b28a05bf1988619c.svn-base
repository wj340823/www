package com.sucsoft.gzhbyjwz.service.user;

import com.sucsoft.gzhbyjwz.service.common.BasicQueryService;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import java.util.HashMap;
import java.util.Map;

@Service
public class LogService {
    @Autowired
    private BasicQueryService basicQueryService;

    public Object listLoginLog(Integer pageNo,Integer pageSize) throws Exception {
        return basicQueryService.queryForPage("listLoginLog",new HashMap<>(), Map.class,pageNo,pageSize);
    }
}
