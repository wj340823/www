<div  ng-controller="yilanbiao">
    <div class="tab-task">
        <div class="tab-nav">
            <button class="tab-button tab-left" ui-sref="msgManage.companyIntro" ui-sref-active="active">
                公司概况
            </button>
            <button class="tab-button tab-left" ui-sref="msgManage.hgsjt" ui-sref-active="active">
                宏观数据图
            </button>
            <button class="tab-button tab-left" ui-sref="msgManage.hjfxys" ui-sref-active="active">
                环境风险要素
            </button>
            <button class="tab-button tab-left" ui-sref="msgManage.hjfxwz" ui-sref-active="active">
                环境风险物质
            </button>
            <button class="tab-button tab-left" ui-sref="msgManage.hjfxdy" ui-sref-active="active">
                环境风险单元
            </button>
            <button class="tab-button tab-left" ui-sref="msgManage.hjfxfkss" ui-sref-active="active">
                环境风险防控设施
            </button>
            <button class="tab-button tab-left" ui-sref="msgManage.sczzq" ui-sref-active="active">
                生产装置区
            </button>
            <button class="tab-button tab-left" ui-sref="msgManage.hjbhmb" ui-sref-active="active">
                环境风险受体
            </button>
            <button class="tab-button tab-left" ui-sref="msgManage.hjwrsgy" ui-sref-active="active" >
                环境污染事故源
            </button>
            <button class="tab-button tab-left" ui-sref="msgManage.tfhjsjqjfx" ui-sref-active="active" >
                突发环境事件情景分析
            </button>
            <div class="tab-button tab-left">
                <span>应急信息</span>
                <i class="iconfont icon-webicon215"></i>
                <div>
                    <button class="tab-button tab-left" ui-sref="msgManage.yjwz" ui-sref-active="active">
                        应急物资
                    </button>
                    <button class="tab-button tab-left" ui-sref="msgManage.yjzhtxl" ui-sref-active="active">
                        应急指挥通讯录
                    </button>
                    <button class="tab-button tab-left" ui-sref="msgManage.yjwbllb" ui-sref-active="active">
                        应急外部联络表
                    </button>
                    <button class="tab-button tab-left" ui-sref="msgManage.yjzj" ui-sref-active="active">
                        应急专家
                    </button>
                </div>
            </div>
        </div>
    </div>
    <br/>
    <br/>
    <div class="table-content">
        <div class="clearfix" style="background-color: #222;height:35px;">
            <div class="tb-title">应急池</div>
        </div>
        <div style="margin-top: 10px;">
            <span class="check-result1">容量(m³)：</span>
            <input class="text-info-input" ng-model="ylb.riskmeasures.poolNumber" onkeyup="this.value=this.value.replace(/[^0-9-]+/,'');" />
        </div>
        <br>
        <div style="margin-top: 10px;">
            <span class="check-result1">是否空置：</span>
            <input type="radio" ng-model="ylb.riskmeasures.sfkz" value="是" class="regular-radio" id="radio1"/>
            <label for="radio1" class="hander"></label>
            <span style="margin-right: 10px">是</span>
            <input type="radio" ng-model="ylb.riskmeasures.sfkz" value="否" class="regular-radio" id="radio2"/>
            <label for="radio2" class="hander"></label>
            <span style="margin-right: 10px">否</span>
        </div>
        <br>
        <div>
            <span class="check-result1" style="width:300px;">厂区事故废液或消防水是否能自流入应急池：</span>
            <input type="radio" ng-model="ylb.riskmeasures.sfzclr" value="是" class="regular-radio" id="radio3"/>
            <label for="radio3" class="hander"></label>
            <span style="margin-right: 10px">是</span>
            <input type="radio" ng-model="ylb.riskmeasures.sfzclr" value="否" class="regular-radio" id="radio4"/>
            <label for="radio4" class="hander"></label>
            <span style="margin-right: 10px">否</span>
        </div>
        <br/>
        <div>
            <span class="check-result1" style="width:300px;">是否有足够能力的排水管和泵全部收集：</span>
            <input type="radio" ng-model="ylb.riskmeasures.sfqbsj" value="是" class="regular-radio" id="radio5"/>
            <label for="radio5" class="hander"></label>
            <span style="margin-right: 10px">是</span>
            <input type="radio" ng-model="ylb.riskmeasures.sfqbsj" value="否" class="regular-radio" id="radio6"/>
            <label for="radio6" class="hander"></label>
            <span style="margin-right: 10px">否</span>
        </div>
        <br/>
        <div>
            <span class="check-result1" style="width:300px;">是否设置初期雨水收集池：</span>
            <input type="radio" ng-model="ylb.riskmeasures.sfsjys" value="是" class="regular-radio" id="radio7"/>
            <label for="radio7" class="hander"></label>
            <span style="margin-right: 10px">是</span>
            <input type="radio" ng-model="ylb.riskmeasures.sfsjys" value="否" class="regular-radio" id="radio8"/>
            <label for="radio8" class="hander"></label>
            <span style="margin-right: 10px">否</span>
        </div>
        <br/>
        <div class="clearfix" style="background-color: #222;height:35px;">
            <div class="tb-title">排水管道</div>
        </div>
        <div style="margin-top: 10px;">
            <span class="check-result1" style="width: 190px;">是否实现雨污分流：</span>
            <input type="radio" ng-model="ylb.riskmeasures.sffywfl" value="是" class="regular-radio" id="radio9"/>
            <label for="radio9" class="hander"></label>
            <span style="margin-right: 10px">是</span>
            <input type="radio" ng-model="ylb.riskmeasures.sffywfl" value="否" class="regular-radio" id="radio10"/>
            <label for="radio10" class="hander"></label>
            <span style="margin-right: 10px">否</span>
        </div>
        <br>
        <div>
            <span class="check-result1" style="width: 190px;">是否设置生产废水总排放阀：</span>
            <input type="radio" ng-model="ylb.riskmeasures.sfszfszpff" value="是" class="regular-radio" id="radio11"/>
            <label for="radio11" class="hander"></label>
            <span style="margin-right: 10px">是</span>
            <input type="radio" ng-model="ylb.riskmeasures.sfszfszpff" value="否" class="regular-radio" id="radio12"/>
            <label for="radio12" class="hander"></label>
            <span style="margin-right: 10px">否</span>
        </div>
        <br/>
        <div>
            <span class="check-result1" style="width: 190px;">否设置雨水总排放阀：</span>
            <input type="radio" ng-model="ylb.riskmeasures.sfszyspff" value="是" class="regular-radio" id="radio13"/>
            <label for="radio13" class="hander"></label>
            <span style="margin-right: 10px">是</span>
            <input type="radio" ng-model="ylb.riskmeasures.sfszyspff" value="否" class="regular-radio" id="radio14"/>
            <label for="radio14" class="hander"></label>
            <span style="margin-right: 10px">否</span>
        </div>
        <br/>
        <div class="operate" >
            <button class="searchBtn" ng-click="saveRiskMeasures()">保存</button>
            <div class="clearfix"></div>
        </div>
</div>
