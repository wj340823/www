---
  grammer: SQL
  queries:
    #根据条件查询物资现状
    - name: testTime
      resultType: Map
      statement:
        "
        select
          d.*
        from
          goodslog d
        where
          1=1
        #if($time)
          and d.TIME >:time
        #end
        "
    - name: getcompany
      resultType: Map
      statement:
        "
        select
          a.*
        from
          enterpriseinformation a
        WHERE
          a.COMPANYID not in (
        select
          b.company
        from
          authoxuser b
        where
          b.company is not null)
        "