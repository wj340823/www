



<div style="background: #222">
   	<div class="modal-header">
   		任务指派
   		<span class="fr"><i class="iconfont icon-guanbi" title="关闭" ng-click="list.cancel()"></i></span>
   	</div>
    <div class="modal-body">
		<table>
			<thead>
				<tr>
					<th style="width:15%;">序号</th>
					<th style="width:30%;">企业名称</th>
					<th style="width:20%;">完成状态</th>
				</tr>
			</thead>
		</table>
		<div cgs-slim options="{width:'100%',height:'300px',size:'5px',color:'#C8C8C8'}">
			<table class="wdtable">
				<tbody>
					<tr ng-repeat="item in list.data">
						<td style="width:15%;">{{$index+1}}</td>
						<td style="width:30%;">{{item.companyname}}</td>
						<td style="width:20%;">{{item.status}}</td>
					</tr>
				</tbody>
			</table>
			<div>
				<uib-pagination class="pagination" ng-change="list.getMsg()"
								total-items="list.totalCount"
								ng-model="list.pageNo"
								first-text="first"
								items-per-page="list.pageSize"
								boundary-link-numbers="true"
								direction-links="false" rotate="false" max-size="3">
				</uib-pagination>
			</div>
		</div>
	</div>
</div>
