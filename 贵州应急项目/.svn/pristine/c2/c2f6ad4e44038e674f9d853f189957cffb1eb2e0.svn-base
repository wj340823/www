package com.sucsoft.gzhbyjwz.bean.constant;

import java.util.HashMap;
import java.util.Map;

public class PreplanConstant {
    /**
     *预案风险级别：1-一般，2-重大，3-较大
     */
    public static final Map levelMap = new HashMap();
    static {
        levelMap.put(1,"一般");
        levelMap.put(2,"重大");
        levelMap.put(3,"较大");
    };

    public enum RecordStatus{
        /**
         * 备案状态：0-未备案,1-已备案，2-即将到期，3-备案超期
         *
         */
        notOnRecord("未备案",0),recorded("已备案",1),aboutToExpire("即将到期",2),expired("备案超期",3);

        private String name;
        private Integer status;

        RecordStatus(String name, Integer status) {
            this.name = name;
            this.status = status;
        }

        public String getName() {
            return name;
        }

        public void setName(String name) {
            this.name = name;
        }

        public Integer getStatus() {
            return status;
        }

        public void setStatus(Integer status) {
            this.status = status;
        }
    }
}
