
<div class="mainOuter" ng-controller="emerStatusCtrl">
    <ul class="homePage">
        <li ui-sref="home.dataType1"><i class="iconfont icon-fengxianyuan"></i> 风险源状况</li>
        <li ui-sref="home.emerStatus1" class="active"><i class="iconfont icon-yingjiwuzi"></i> 应急物资状况</li>
        <li ui-sref="home.organise"><i class="iconfont icon-jigourenyuan"></i> 机构人员</li>
        <li ui-sref="home.dealing"><i class="iconfont icon-chuli"></i> 处置方式状况</li>
        <!--<li  ui-sref="home.waterSource"><i class="iconfont icon-minganci"></i> 环境敏感点状况</li>-->
    </ul>
    <div class="prePage">
        <div class="prePageBtn"  ui-sref="home.emerStatus1">
            <i class="iconfont icon-next-page-copy"></i>
        </div>
    </div>
    <div style="" class="emerSub">
        <div class="item-title" style="margin-top: 0">厂区存储应急物资检索</div>
        <div class="content">
            <div class="theme">名称搜索</div>
            <div>
                <input type="text" placeholder="请输入名称" ng-model="inputSub" />
                <button type="button" class="searchBtn" ng-click="searchSub(inputSub)"><i class="iconfont icon-sousuo"></i>&nbsp;搜索</button>
            </div>
            <div class="theme">搜索结果</div>
            <div cgs-slim options="{width:'100%',height:'285px',size:'5px',color:'#C8C8C8'}">
                <div ng-repeat="item in searchSubResult" class="riskComItem cursor-pointer" style="margin:5px 0" ng-click="locateSource(item)">
                    <span>{{item.COMPANYNAME}}</span>
                </div>
            </div>
        </div>
    </div>
</div>
