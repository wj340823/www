<div  ng-controller="yilanbiao">
    <div class="clearfix">
        <div class="tb-title"> 环境风险受体-新增</div>
        <div class="fr">
            <button class="searchBtn" onclick="history.go(-1)">
                <i class="iconfont icon-fanhui"></i>
                返回
            </button>
        </div>
    </div>
    <div class="item-content" style="background-color: rgba(0,0,0,.7)">
        <div>
            <span class="check-result1">类型：</span>
            <input type="radio" ng-model="ylb.td2.bhlx" value="水" class="regular-radio" id="radio1" />
            <label for="radio1" class="hander"></label>
            <span style="margin-right: 10px">水环境保护目标 </span>
            <input type="radio" ng-model="ylb.td2.bhlx" value="大气" class="regular-radio" id="radio2"/>
            <label for="radio2" class="hander"></label>
            <span style="margin-right: 10px">气环境保护目标</span>
            <br><br>
        </div>
        <div>
            <span class="check-result1">保护目标：</span>
            <input class="text-info-small" ng-model="ylb.td2.protectionObjectives" >
            <br><br>
        </div>
        <br>
        <div>
            <span class="check-result1">方位：</span>
            <input class="text-info-small" ng-model="ylb.td2.position" >
            <br><br>
        </div>
        <br>
        <div>
            <span class="check-result1">距离(m)：</span>
            <input class="text-info-small" ng-model="ylb.td2.distance" >
            <br><br>
        </div>
        <br>
        <div>
            <span class="check-result1">人数/户数：</span>
            <input class="text-info-small" ng-model="ylb.td2.households">
            <br><br>
        </div>
        <br>
        <div>
            <span class="check-result1">保护目的：</span>
            <input class="text-info-small" ng-model="ylb.td2.objective">
            <br><br>
        </div>
        <br>
        <div>
            <span class="check-result1">标准：</span>
            <input class="text-info-small" ng-model="ylb.td2.standard">
            <br><br>
        </div>
        <br>
        <div class="operate" >
            <button class="searchBtn" ng-click="items.type=='edit'?editYlb(2):postYlb(2)">保存</button>
            <div class="clearfix"></div>
        </div>
    </div>

    <div class="task-alert">
        <div class="alert-title">操作提示</div>
        <div class="alert-content">{{textContent}}</div>
        <ul class="alert-button">
            <li ng-click="sureAlert()" ng-if="textContent=='确定提交吗?'">确定</li>
            <li ng-click="resetAlert(textContent)">{{textContent=='确定提交吗?'?'取消':'关闭'}}</li>
        </ul>
    </div>
</div>
