package com.sucsoft.gzhbyjwz.bean.dc12;

import io.swagger.annotations.ApiModel;
import io.swagger.annotations.ApiModelProperty;

@ApiModel(value = "应急预案", description = "应急预案")
public class EmergencyPlan {

	private String emergencyPlanId;
	private String companyId;//所属公司id
	@ApiModelProperty(value = "事故名称")
	private String accidentName;
	@ApiModelProperty(value = "事故级别")
	private String accidentLevel;
	@ApiModelProperty(value = "预警措施")
	private String forewarningMeasures;
	@ApiModelProperty(value = "应急响应流程")
	private String emergencyProcess;
	@ApiModelProperty(value = "应急措施")
	private String emergencyMeasure;

	public String getEmergencyPlanId() {
		return emergencyPlanId;
	}
	public void setEmergencyPlanId(String emergencyPlanId) {
		this.emergencyPlanId = emergencyPlanId;
	}
	public String getCompanyId() {
		return companyId;
	}
	public void setCompanyId(String companyId) {
		this.companyId = companyId;
	}
	public String getAccidentName() {
		return accidentName;
	}
	public void setAccidentName(String accidentName) {
		this.accidentName = accidentName;
	}
	public String getAccidentLevel() {
		return accidentLevel;
	}
	public void setAccidentLevel(String accidentLevel) {
		this.accidentLevel = accidentLevel;
	}
	public String getForewarningMeasures() {
		return forewarningMeasures;
	}
	public void setForewarningMeasures(String forewarningMeasures) {
		this.forewarningMeasures = forewarningMeasures;
	}
	public String getEmergencyProcess() {
		return emergencyProcess;
	}
	public void setEmergencyProcess(String emergencyProcess) {
		this.emergencyProcess = emergencyProcess;
	}
	public String getEmergencyMeasure() {
		return emergencyMeasure;
	}
	public void setEmergencyMeasure(String emergencyMeasure) {
		this.emergencyMeasure = emergencyMeasure;
	}
}
