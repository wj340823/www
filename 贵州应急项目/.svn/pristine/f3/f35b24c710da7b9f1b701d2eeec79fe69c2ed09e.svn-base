    <script type="text/javascript">
        //地图加载
        function Init() {
            try {
                var flyPath = "D:/guizhoufly/guizhou.fly";
                SGWorld.attachEvent("OnLoadFinished", OnProjectLoadFinished);
                SGWorld.Project.Open(flyPath);
            } catch (e) {
                alert("Error: " + e.description);
            }
        }

        try{
            navigator.geolocation.getCurrentPosition(function(position) {
                longitude = position.coords.longitude;
                latitude = position.coords.latitude;
            })
        }catch(e){
        }

        function OnProjectLoadFinished() {

            //初始化地图显示位置

            var lat,lon,companyId;
            var arr = window.location.search.slice(4).split('&');
            companyId = arr[0]; lon = arr[1]; lat = arr[2];
            var position = SGWorld.Creator.CreatePosition(lon, lat, 4000, 0,
                55.999944, -75.000728, 0, 0);
            SGWorld.Navigate.FlyTo(position);
            initRoot();
        }
    </script>

</head>

