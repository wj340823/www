
package com.sucsoft.gzhbyjwz.bean.dc12;


public class ProtectionObject {

    /**
     * 标准
     * 
     */
    private String standard;
    private String companyId;
    /**
     * 距离
     * 
     */
    private String distance;
    /**
     * 人数/户数
     * 
     */
    private String peopleNumber;
    /**
     * 保护目的
     * 
     */
    private String purpose;
    private String protectionobjectsId;
    /**
     * 保护目标
     * 
     */
    private String protectObject;
    /**
     * 方位
     * 
     */
    private String direction;

    public void setStandard(String standard) {
        this.standard = standard;
    }

    public String getStandard() {
        return standard;
    }

    public void setCompanyId(String companyId) {
        this.companyId = companyId;
    }

    public String getCompanyId() {
        return companyId;
    }

    public void setDistance(String distance) {
        this.distance = distance;
    }

    public String getDistance() {
        return distance;
    }

    public void setPeopleNumber(String peopleNumber) {
        this.peopleNumber = peopleNumber;
    }

    public String getPeopleNumber() {
        return peopleNumber;
    }

    public void setPurpose(String purpose) {
        this.purpose = purpose;
    }

    public String getPurpose() {
        return purpose;
    }

    public void setProtectionobjectsId(String protectionobjectsId) {
        this.protectionobjectsId = protectionobjectsId;
    }

    public String getProtectionobjectsId() {
        return protectionobjectsId;
    }

    public String getProtectObject() {
        return protectObject;
    }

    public void setProtectObject(String protectObject) {
        this.protectObject = protectObject;
    }

    public void setDirection(String direction) {
        this.direction = direction;
    }

    public String getDirection() {
        return direction;
    }

}
