---
  grammer: HQL
  queries:
    #根据预案id查询附件列表
    - name: getPlanList
      resultType: EnterprisePreplan
      statement:
        "
        FROM
          EnterprisePreplan
        where
          p.deleted = 0
        #if($companyId)
          and companyId = :companyId
        #end
        "
    #根据预案id查询附件列表
    - name: getInfoList
      resultType: EnterprisePreplan
      statement:
        "
        FROM
          EnterprisePreplan
        where
          shzt < 3
        #if($companyId)
          and companyId = :companyId
        #end
        "
#        #if($deleted)
#          and deleted = :deleted
#        #end