    public void deal(String s1,String s2,Map infoMap,Map mgdMap,Map map1,Map map2){
        JsonParser parser=new JsonParser();
        JsonObject object= null;
        object = (JsonObject) parser.parse(s1);
        JsonObject data  = (JsonObject)object.get("data");
        JsonElement zcqkl=data.get("zcqk");
        if(zcqkl != null){
            JsonArray zcqk =zcqkl.getAsJsonArray();
            for (int i=0;i<zcqk.size();i++) {
                JsonObject info = (JsonObject) zcqk.get(i).getAsJsonObject().get("info");
                String pm = info.get("pm").toString();
                String cas = info.get("cas").toString();
                String ljl = info.get("ljl").toString();
                List<String> list = new ArrayList<>();
                list.add(pm);
                list.add(cas);
                list.add(ljl);
                infoMap.put(pm, list);
            }
        }
        JsonArray mgd=data.get("mgd").getAsJsonArray();
        List<String> mgds = new ArrayList<>();
        for(int i= 0;mgd.size()>i;i++){
            String name = mgd.get(i).getAsJsonObject().get("name").toString();
            mgds.add(name);
        }
        map1.put(s2,mgds);
        JsonArray factory=data.get("factory").getAsJsonArray();
        List<String> factorys = new ArrayList<>();
        for(int i= 0;factory.size()>i;i++){
            String name = factory.get(i).getAsJsonObject().get("name").toString();
            factorys.add(name);
        }
        map2.put(s2,factorys);
        JsonArray mgdMarkers = data.get("mgdMarkers").getAsJsonArray();
        JsonArray mgdInfo = data.get("mgdinfo").getAsJsonArray();
        for(int i=0;i<mgdMarkers.size();i++){
            JsonObject info1 = mgdMarkers.get(i).getAsJsonObject();
            JsonObject info2 = (JsonObject)mgdInfo.get(i).getAsJsonObject().get("info");
            String LONGITUDE = info1.get("LONGITUDE").toString();
            String LATITUDE = info1.get("LATITUDE").toString();
            String COMPANYNAME = info1.get("COMPANYNAME").toString();
            String TYPE = info1.get("TYPE").toString();
            String fw = info2.get("fw").toString();
            String jl = info2.get("jl").toString();
            String rs = info2.get("rs").toString();
            List<String> list = new ArrayList<>();
            list.add(LONGITUDE);
            list.add(LATITUDE);
            list.add(COMPANYNAME);
            list.add(TYPE);
            list.add(fw);
            list.add(jl);
            list.add(rs);
            mgdMap.put(COMPANYNAME,list);
        }

    }

