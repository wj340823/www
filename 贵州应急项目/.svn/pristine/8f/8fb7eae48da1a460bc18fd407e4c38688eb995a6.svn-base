package com.sucsoft.gzhbyjwz.bean.common;

public class VsResultBean {
	private static final Integer DO_DATA_SUCCESS = 0;
//	private static final Integer DO_DATA_FAIL = 1;
	
	
	public VsResultBean() {
		super();
	}
	
	public VsResultBean(Integer result, Object msg, Object data) {
		super();
		this.result = result;
		this.msg = msg;
		this.data = data;
	}

	public VsResultBean(Integer result, Object msg) {
		super();
		this.result = result;
		this.msg = msg;
	}

	private Integer result = DO_DATA_SUCCESS;
	private Object msg;
	private Object data;
	public Integer getResult() {
		return result;
	}
	public void setResult(Integer result) {
		this.result = result;
	}
	public Object getMsg() {
		return msg;
	}
	public void setMsg(Object msg) {
		this.msg = msg;
	}
	public Object getData() {
		return data;
	}
	public void setData(Object data) {
		this.data = data;
	}
	

	
}
