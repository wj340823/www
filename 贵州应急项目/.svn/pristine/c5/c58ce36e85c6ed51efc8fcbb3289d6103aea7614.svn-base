package com.sucsoft.gzhbyjwz.bean.dc12;

import io.swagger.annotations.ApiModel;
import io.swagger.annotations.ApiModelProperty;

@ApiModel(value = "应急小组", description = "应急小组")
public class EmergencyGroup {

	@ApiModelProperty(value = "唯一标识")
	private String emergencyGroupId;
	@ApiModelProperty(value = "组名称")
	private String groupName;
	@ApiModelProperty(value = "所属单位")
	private String theUtil;
	@ApiModelProperty(value = "组成员")
	private String groupPeople;
	@ApiModelProperty(value = "联系方式")
	private String telephone;
	@ApiModelProperty(value = "小组职能")
	private String groupFunction;
	private String companyId;
	public String getEmergencyGroupId() {
		return emergencyGroupId;
	}
	public void setEmergencyGroupId(String emergencyGroupId) {
		this.emergencyGroupId = emergencyGroupId;
	}
	public String getGroupName() {
		return groupName;
	}
	public void setGroupName(String groupName) {
		this.groupName = groupName;
	}
	public String getTheUtil() {
		return theUtil;
	}
	public void setTheUtil(String theUtil) {
		this.theUtil = theUtil;
	}
	public String getGroupPeople() {
		return groupPeople;
	}
	public void setGroupPeople(String groupPeople) {
		this.groupPeople = groupPeople;
	}
	public String getTelephone() {
		return telephone;
	}
	public void setTelephone(String telephone) {
		this.telephone = telephone == null?null:telephone.replaceAll(",", "");
	}
	public String getGroupFunction() {
		return groupFunction;
	}
	public void setGroupFunction(String groupFunction) {
		this.groupFunction = groupFunction;
	}
	public String getCompanyId() {
		return companyId;
	}
	public void setCompanyId(String companyId) {
		this.companyId = companyId;
	}

}
