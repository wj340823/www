    <table style="margin-top: 10px">
      <thead>
            <tr>
              <th>序号</th>
              <th>企业名称</th>
              <th>风险级别</th>
              <th>备案时间</th>
              <th>审核状态</th>
              <th>操作</th>
            </tr>
          </thead>
          <tbody>
            <tr ng-repeat="detail in page.list.dataList">
              <td ng-bind="$index+1">1</td>
              <td> {{ detail.COMPANYNAME }} </td>
              <td> {{ detail.RISKGRADE=="重大"?'重大风险源': detail.RISKGRADE=="较大"?'较大风险源':detail.RISKGRADE=="一般"?'一般风险源':''}} </td>
              <td> {{ detail.RECORDTIME | date:'yyyy-MM-dd'}} </td>
              <td ng-class="detail.RECORDSTATUS==3?'dangerColor':detail.RECORDSTATUS==1?'finishColor':detail.RECORDSTATUS==2?
                  'warning':''"> {{ detail.RECORDSTATUS==3?'驳回':detail.RECORDSTATUS==2?'审核中':detail.RECORDSTATUS==1?'已完成':
                detail.RECORDSTATUS==0?'未备案':'未备案'}} </td>
              <td ui-sref-active="active" ui-sref="enterpriseHead.emergencyDetail({ID:detail.ID, RECORDTIME:detail.RECORDTIME, COMPANYNAME:detail.COMPANYNAME, RECORDSTATUS:detail.RECORDSTATUS, LEVEL:detail.LEVEL, UNDERSIGNED:detail.UNDERSIGNED})">
                <span class="hander oper">详情</span>
              </td>
            </tr>
