<div class="mainOuter" ng-controller="indexCtrl">
  <div class="header">
    <div class="logo">
      <img src="images/lgfxy.png" alt="">
    </div>
    <div class="userInfo" >
      <span>{{userName}}</span>
      <i class="iconfont icon-webicon215" style="font-size: 12px;"></i>
      <div class="logout">
        <a href="logout">注销登录</a>
        <a ng-click="showUserInfo()">用户信息</a>
        <a ng-click="changePwd()">修改密码</a>
      </div>
    </div>
    <ul class="menu" style="margin-right: 15px">
      <li ui-sref="index" ui-sref-active="active">
        <i class="iconfont icon-shouye"></i> <span>首页</span>
      </li>
      <li ui-sref="home" ui-sref-active="active">
        <i class="iconfont icon-shujuzhongxin"></i> <span>数据中心</span>
      </li>
      <li>
        <div>
          <i class="iconfont icon-wuzi"></i>
          <span>应急资源</span>
        </div>
        <div class="menuSecond">
          <div ui-sref="emergency.emergencyMaterial" ui-sref-active="active">应急物资</div>
          <div ui-sref="emergencyPlan" ui-sref-active="active">应急预案</div>
          <div ui-sref="emergency.emerCase" ui-sref-active="active">应急案例</div>
          <!--<div ui-sref="emergency.organise" ui-sref-active="active">机构人员</div>-->
          <div ui-sref="emergency.experts" ui-sref-active="active">专家库</div>
        </div>
      </li>
      <li ui-sref="riskPrevention.riskSource" ui-sref-active="active">
        <i class="iconfont icon-fengxianyuan"></i> <span>风险源</span>
      </li>
      <li ui-sref="province" ui-sref-active="active">
        <i class="iconfont icon-fengxianpaicha"></i> <span>风险排查</span>
      </li>
      <li>
        <div>
          <i class="iconfont icon-kongjianfenxi"></i>
          <span>风险评估</span>
        </div>
        <div class="menuSecond">
          <div ui-sref="riskPrevention.riskSpatial.resouceRelation" ui-sref-active="active">源关系</div>
          <div ui-sref="riskPrevention.riskSpatial.emergencyMng" ui-sref-active="active">应急管理</div>
          <div ng-if="userList.role!='qyrole'" ui-sref="riskPrevention.riskSpatial.air" ui-sref-active="active">气模型工具</div>
          <div ng-if="userList.role!='qyrole'" ui-sref="riskPrevention.riskSpatial.water" ui-sref-active="active">水模型工具</div>
        </div>
      </li>
      <li ng-show="userList.role=='admin'">
        <div ui-sref="system.user"  ui-sref-active="active">
          <i class="iconfont icon-set"></i>
          <span>系统管理</span>
        </div>
      </li>
    </ul>
  </div>
  <div class="indexCon">
    <div class="centerCon">
      <div class="leftCon ">
        <div class="conTitle">数据中心</div>
        <div class="leftIn1 inDiv" ui-sref="home">
          <div class="coreDiv" >
            <i class="iconfont icon-shujuzhongxin" style="font-size: 60px"></i>
            <span>数据中心</span>
          </div>
        </div>
        <div class="conTitle">应急资源</div>
        <div class="leftIn2" >
          <div class="leftInyjzy " >
            <div class="leftInyjzy-l inDiv"  ui-sref="emergency.emergencyMaterial">
              <div class="coreDiv" style="height:80px;top:calc(50% - 40px)">
                <i class="iconfont icon-fengxianpaicha" style="font-size: 27px"></i>
                <span style="margin-top: 10%;font-size: 14px;font-weight: 200">应急物资</span>
              </div>
            </div>
            <div class="leftInyjzy-r inDiv" ui-sref="emergencyPlan">
              <div class="coreDiv" style="height:80px;top:calc(50% - 40px)">
                <i class="iconfont icon-renwuyujing" style="font-size: 28px"></i>
                <span style="margin-top: 10%;font-size: 14px;font-weight: 200">应急预案</span>
              </div>
            </div>
          </div>
          <div class="leftInyjzy" >
            <div class="leftInyjzy-l inDiv" ui-sref="emergency.emerCase">
              <div class="coreDiv" style="height:80px;top:calc(50% - 40px)">
                <i class="iconfont icon-gongzuobaobiao" style="font-size: 28px"></i>
                <span style="margin-top: 10%;font-size: 14px;font-weight: 200">应急案例</span>
              </div>
            </div>
            <div class="leftInyjzy-r inDiv" ui-sref="emergency.experts">
              <div class="coreDiv" style="height:80px;top:calc(50% - 40px)">
                <i class="iconfont icon-zhuanjia" style="font-size: 28px"></i>
                <span style="margin-top: 10%;font-size: 14px;font-weight: 200">专家库</span>
              </div>
            </div>
          </div>
          <!--<div class="leftInyjzy">
            <div class="leftInyjzy-l inDiv"   style="width: 100%">
              <div class="coreDiv" style="height:80px;top:calc(50% - 40px)">
                <i class="iconfont " style="font-size: 32px"></i>
                <span style="margin-top: 10%;font-size: 14px;font-weight: 200">专家库</span>
              </div>
            </div>
          </div>-->
        </div>
      </div>
      <div class="midCon ">
        <div class="conTitle">风险防控</div>
        <div class="fxyIn inDiv" ui-sref="riskPrevention.riskSource">
          <div class="coreDiv" style="height:100px;top:calc(50% - 50px)">
            <i class="iconfont icon-fengxianyuan" style="font-size: 50px"></i>
            <span style="margin-top: 10px;font-size: 16px">风险源</span>
          </div>
        </div>
        <div class="fxyIn inDiv" ui-sref="province">
          <div class="coreDiv" style="height:100px;top:calc(50% - 50px)">
            <i class="iconfont icon-guzhangpaicha" style="font-size: 50px"></i>
            <span style="margin-top: 10px;font-size: 16px">风险排查</span>
          </div>
        </div>
        <div class="fxyIn inDiv" style="margin-bottom: 0" ui-sref="riskPrevention.riskSpatial.resouceRelation">
          <div class="coreDiv" style="height:100px;top:calc(50% - 50px)">
            <i class="iconfont icon-icon-test" style="font-size: 50px"></i>
            <span style="margin-top: 10px;font-size: 16px;">风险评估</span>
          </div>
        </div>
      </div>
      <div class="rightCon">
        <div class="conTitle">信息检索</div>
        <div class="searchIn inDiv" ui-sref="riskPrevention.riskSource({search:true})">
          <div class="coreDiv" >
            <i class="iconfont icon-chazhaofengxianyuan" style="font-size: 60px"></i>
            <span>找风险源</span>
          </div>
        </div>
        <div class="searchIn inDiv" style="margin-bottom: 0" ui-sref="emergency.emergencyMaterial({search:true})">
          <div class="coreDiv" >
            <i class="iconfont icon-chazhaoyingjiziyuan" style="font-size: 60px"></i>
            <span>找应急物资</span>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
