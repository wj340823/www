<!-- 一览表-->
<div  style="height: 100%" class="overviewCon" >
	<button class="tab-button" ng-click="showYlb('intro')" ng-class="company.intro?'active':''">公司概况</button>
	<button class="tab-button" ng-click="showYlb('hgsjt')" ng-class="company.hgsjt?'active':''">宏观数据图</button>
	<button class="tab-button" ng-click="showYlb('hjfxys')" ng-class="company.hjfxys?'active':''">环境风险要素</button>
	<button class="tab-button" ng-click="showYlb('hjfxwz')" ng-class="company.hjfxwz?'active':''">环境风险物质</button>
	<button class="tab-button" ng-click="showYlb('hjfxdy')" ng-class="company.hjfxdy?'active':''">环境风险单元</button>
	<button class="tab-button" ng-click="showYlb('hjfxfkss')" ng-class="company.hjfxfkss?'active':''">环境风险防控设施</button>
	<button class="tab-button" ng-click="showYlb('sczz')" ng-class="company.sczz?'active':''">生产装置区</button>
	<button class="tab-button" ng-click="showYlb('scgy')" ng-class="company.scgy?'active':''">生产工艺</button>
	<button class="tab-button" ng-click="showYlb('hjbhmb')" ng-class="company.hjbhmb?'active':''">环境风险受体</button>
	<button class="tab-button" ng-click="showYlb('hjwrsgy')" ng-class="company.hjwrsgy?'active':''">环境污染事故源</button>
	<button class="tab-button" ng-click="showYlb('tfhjsjqjfx')" ng-class="company.tfhjsjqjfx?'active':''">突发环境事件情景分析</button>
	<button class="tab-button" ng-click="showYlb('yjwz')" ng-class="company.yjwz?'active':''">应急物资</button>
	<button class="tab-button" ng-click="showYlb('yjzhtxl')" ng-class="company.yjzhtxl?'active':''">应急指挥通讯录</button>
	<button class="tab-button" ng-click="showYlb('yjwbllb')" ng-class="company.yjwbllb?'active':''">应急外部联络表</button>
	<button class="tab-button" ng-click="showYlb('yjzj')" ng-class="company.yjzj?'active':''">应急专家</button>
	<div cgs-slim options="{size:'6px',height:'calc(100% - 10px)'}">
		<div id="content">
			<div ng-show="company.intro">
				<div>
					<h5 class="second_title">公司概况</h5>
				</div>
				<div class="companyLeft">
					<div class="leftWord">
						<span>风险源名称：</span>
					</div>
				</div>
				<div class="companyRight">
					<div>{{companyInfoTro.companyName||'&nbsp;'}}</div>
				</div>
				<div class="companyLeft">
					<div class="leftWord">
						<span>企业名称：</span>
					</div>
				</div>
				<div class="companyRight">
					<div>{{companyInfoTro.companyName||'&nbsp;'}}</div>
				</div>
				<div class="companyLeft">
					<div class="leftWord">
						<span>简介：</span>
					</div>
				</div>
				<div class="companyRight">
					<div>{{companyInfoTro.simpleIntroduce||'&nbsp;'}}</div>
				</div>
				<div class="companyLeft">
					<div class="leftWord">
						<span>地址：</span>
					</div>
				</div>
				<div class="companyRight">
					<div>{{companyInfoTro.address||'&nbsp;'}}</div>
				</div>
				<div class="companyLeft">
					<div class="leftWord">
						<span>交通情况：</span>
					</div>
				</div>
				<div class="companyRight">
					<div>{{companyInfoTro.transport||'&nbsp;'}}</div>
				</div>
				<div class="itemLine">
					<div class="threearea">
						<span class="onlyone">行业类别代码：</span>
						<span class="addipt ng-binding">{{companyInfoTro.industryCategoryCode}}</span>
					</div>
					<div class="threearea">
						<span>经度：</span>
						<span class="addipt ng-binding">{{companyInfoTro.longitude}}</span>
					</div>
					<div class="threearea">
						<span>纬度：</span>
						<span class="addipt ng-binding">{{companyInfoTro.latitude}}</span>
					</div>
				</div>
				<div class="itemLine">
					<div class="threearea">
						<span class="onlyone">行业类别名称：</span>
						<span class="addipt ng-binding">{{companyInfoTro.industryCategoryName}}</span>
					</div>
					<div class="threearea">
						<span>法人：</span>
						<span class="addipt ng-binding">{{companyInfoTro.legalRepresentative}}</span>
					</div>
					<div class="threearea">
						<span>统一社会信用代码：</span>
						<span class="addipt ng-binding">{{companyInfoTro.socialCreditCode}}</span>
					</div>
				</div>
				<div class="itemLine">
					<div class="threearea">
						<span class="onlyone">企业规模：</span>
						<span class="addipt ng-binding">{{companyInfoTro.enterpriseSize}}</span>
					</div>
					<div class="threearea">
						<span>建厂时间：</span>
						<span class="addipt ng-binding">{{companyInfoTro.documentYear}}</span>
					</div>
					<div class="threearea">
						<span>厂区面积：</span>
						<span class="addipt ng-binding">{{companyInfoTro.riskSourceArea}}</span>
					</div>
				</div>
				<div class="itemLine">
					<div class="threearea">
						<span class="onlyone">企业负责人：</span>
						<span class="addipt ng-binding">{{companyInfoTro.legalPerson}}</span>
					</div>
					<div class="threearea">
						<span>企业负责人联系电话：</span>
						<span class="addipt ng-binding">{{companyInfoTro.enterpriseHeadPhone}}</span>
					</div>
					<div class="threearea">
						<span>传真电话：</span>
						<span class="addipt ng-binding">{{companyInfoTro.onDutyTelephone}}</span>
					</div>
				</div>
				<div class="itemLine">
					<div class="threearea">
						<span class="onlyone">填报人姓名：</span>
						<span class="addipt ng-binding">{{companyInfoTro.custodian}}</span>
					</div>
					<div class="threearea">
						<span>填报人联系电话：</span>
						<span class="addipt ng-binding">{{companyInfoTro.custodianTelephone}}</span>
					</div>
					<div class="threearea">
						<span>电子邮箱：</span>
						<span class="addipt ng-binding">{{companyInfoTro.email}}</span>
					</div>
				</div>
				<!-- ngRepeat: item in cpclInfo track by $index -->
				<div class="itemLine ng-scope" style="clear: both;" ng-repeat="item in companyInfoTro.productInfos">
					<div class="threearea">
						<span class="onlyone">主要产品：</span>
						<span class="addipt ng-binding">{{item.name}}</span>
					</div>
					<div class="threearea">
						<span>产量：</span>
						<span class="addipt ng-binding">{{item.number}}</span>
					</div>
				</div>
				<div class="clearfix"></div>
<!--				<div>-->
<!--					<h5 class="second_title">厂区平面图</h5>-->
<!--				</div>-->
<!--				<img ng-show="cqpmt==1" src="riskSource/downloadAttachment?companyId={{companyInfoTro.companyId}}&type=1"-->
<!--					 style="width: 90%;margin:10px auto 20px ;display: block">-->
<!--                <p class="noDate dangerColor" ng-show="cqpmt==2">无图片</p>-->
			</div>
			<div ng-if="company.hgsjt" class="ng-scope" style="">
				<div ng-show="cqpmt" class="">
					<div ng-repeat="srcimg in riskfile" class="ng-scope">
						<span style="display: block;text-align: center;" class="ng-binding">{{srcimg.type}}-{{srcimg.fileName}}</span>
						<img class="dimg" style="width: 90%;margin:10px auto 20px ;display: block" src="riskSourceTwo/file/download/{{srcimg.id}}">
					</div>
				</div>
			</div>
			<div ng-if="company.hjfxys" class="compinfo ng-scope" style="">
				<div class="item-content">
					<div class="itemLine">
						<div class="twoarea">
							<span class="sixsix">环境风险等级：</span>
							<span class="addipt osixsix ng-binding">{{riskfactors.riskLeve}}</span>
						</div>
						<div class="twoarea">
							<span class="sixsix">备案号：</span>
							<span class="addipt osixsix ng-binding">{{riskfactors.beiAnNum}}</span>
						</div>
					</div>
					<div class="itemLine">
						<div class="twoarea">
							<span class="sixsix">备案时间：</span>
							<span class="addipt osixsix ng-binding">{{riskfactors.time}}</span>
						</div>
						<div class="twoarea">
							<span class="sixsix">企业所属流域：</span>
							<span class="addipt osixsix ng-binding">{{riskfactors.ssly}}</span>
						</div>
					</div>
					<div class="itemLine">
						<div class="twoarea">
							<span class="sixsix">入河口点位经度：</span>
							<span class="addipt osixsix ng-binding">{{riskfactors.jd}}</span>
						</div>
						<div class="twoarea">
							<span class="sixsix">入河口点位纬度：</span>
							<span class="addipt osixsix ng-binding">{{riskfactors.wd}}</span>
						</div>
					</div>
					<div class="itemLine">
						<div class="twoarea">
							<span class="sixsix">排水去向：</span>
							<span class="addipt osixsix ng-binding">{{riskfactors.psDirection}}</span>
						</div>
						<div class="twoarea">
							<span class="sixsix">是否发生环境突发事件：</span>
							<span class="addipt osixsix ng-binding">{{riskfactors.isTfsj == '1'?'是':'否'}}</span>
						</div>
					</div>
					<div class="itemLine">
						<div class="twoarea">
							<span class="sixsix">是否涉危化品：</span>
							<span class="addipt osixsix ng-binding">{{riskfactors.isWxhxp == '1'?'是':'否'}}</span>
						</div>
						<div class="twoarea">
							<span class="sixsix">是否涉重金属：</span>
							<span class="addipt osixsix ng-binding">{{riskfactors.isZjs == '1'?'是':'否'}}</span>
						</div>
					</div>
					<div class="itemLine">
						<div class="twoarea">
							<span class="sixsix">是否涉危险废物：</span>
							<span class="addipt osixsix ng-binding">{{riskfactors.isWxfw == '1'?'是':'否'}}</span>
						</div>
						<div class="twoarea">
							<span class="sixsix">是否涉尾矿库：</span>
							<span class="addipt osixsix ng-binding">{{riskfactors.isWkk == '1'?'是':'否'}}</span>
						</div>
					</div>
					<div class="itemLine">
						<div class="twoarea">
							<span class="sixsix">是否沿江沿河：</span>
							<span class="addipt osixsix ng-binding">{{riskfactors.isYjyh == '1'?'是':'否'}}</span>
						</div>
						<div class="twoarea">
							<span class="sixsix">是否涉有毒有害气体：</span>
							<span class="addipt osixsix ng-binding">{{riskfactors.isYdyhqt == '1'?'是':'否'}}</span>
						</div>
					</div>
					<div class="itemLine">
						<span class="threeone">是否涉集中式饮用水源上游补给水区域：</span>
						<span class="addipt ng-binding">{{riskfactors.isJzsyysyd == '1'?'是':'否'}}</span>
					</div>
				</div>
			</div>
			<div ng-if="company.hjfxwz" class="ng-scope">
				<div class="item-content">
					<div class="seekarea" style="width: 50%;">
						<span class="fl">类型：</span>
						<div class="radiobox fl">
							<label style="width: 100%;">
								<input ng-model="fxwzlx" value="1" class="regular-radio"  id="fxwztype1"  type="radio"  ng-change="getMsgList()">
								<label for="fxwztype1" class="hander"></label>
								<span class="mlmr" style="margin-right: 12px">水环境</span>
							</label>
						</div>
						<div class="radiobox fl">
							<label style="width: 100%;">
								<input ng-model="fxwzlx" value="2" class="regular-radio"  id="fxwztype2"  type="radio"  ng-change="getMsgList()">
								<label for="fxwztype2" class="hander"></label>
								<span class="mlmr" style="margin-right: 12px">气环境</span>
							</label>
						</div>
					</div>
					<table class="tables">
						<thead>
						<tr>
							<th>序号</th>
							<th>名称</th>
							<th>化学代码</th>
							<th>使用或贮存场所</th>
							<th>最大使用或贮存量(吨)</th>
							<th>临界量(吨)</th>
						</tr>
						</thead>
						<tbody>
						<tr ng-repeat="item in ylb.risksubstances">
							<td >{{items.hjfxwzpageSize*(items.hjfxwzPageNo-1)+$index+1}}</td>
							<td ng-bind="item.name"></td>
							<td ng-bind="item.chemicalCode"></td>
							<td>{{item.place}}</td>
							<td ng-bind="item.storage"></td>
							<td ng-bind="item.ljl"></td>
						</tr>
						</tbody>
					</table>
					<uib-pagination
							ng-change="getMsgList()"
							total-items="storagePage.hjfxwztotalCount"
							ng-model="storagePage.hjfxwzPageNo"
							items-per-page="storagePage.hjfxwzpageSize"
							first-text="first"
							class="pagination"
							boundary-link-numbers="true"
							force-ellipses="true"
							direction-links="false"
							max-size= "3"
							rotate="true">
					</uib-pagination>
					<div class="clearfix"></div>
				</div>
			</div>
			<div ng-if="company.hjfxdy" class="ng-scope">
				<div class="item-content">
					<table class="tables">
						<thead>
						<tr>
							<th>序号</th>
							<th>名称</th>
							<th>场所</th>
							<th>环境风险物资</th>
							<th>最大贮存量(m³)</th>
							<th>是否有围堰</th>
							<th>是否防漏</th>
							<th>是否有应急池/污水处理设施</th>
							<th>是否有喷淋措施和预警监测</th>
						</tr>
						</thead>
						<tbody>
							<tr ng-repeat="item in riskunit" class="ng-scope">
								<td ng-bind="$index+1"></td>
								<td ng-bind="item.riskName"></td>
								<td>{{item.address}}</td>
								<td ng-bind="item.unitName"></td>
								<td ng-bind="item.maxNumber"></td>
								<td ng-bind="item.sfwy"></td>
								<td ng-bind="item.sffs"></td>
								<td ng-bind="item.sfff"></td>
								<td ng-bind="item.sfwscl"></td>
								<td ng-bind="item.sfqtcl"></td>
							</tr>
						</tbody>
					</table>
				</div>
			</div>
			<div ng-if="company.hjfxfkss" class="compinfo ng-scope" style="">
				<div class="item-content">
					<div class="seekarea" style="width: 50%;">
						<span class="fl">类型：</span>
						<div class="radiobox fl">
							<label style="width: 100%;">
								<input ng-model="fksslx" value="1" class="none-radio ng-pristine ng-untouched ng-valid ng-not-empty" id="fksstype1" name="1494" type="radio">
								<label for="fksstype1" class="hander"></label>
								<span class="mlmr" style="margin-right: 12px">水环境</span>
							</label>
						</div>
						<div class="radiobox fl">
							<label style="width: 100%;">
								<input ng-model="fksslx" value="2" class="none-radio ng-pristine ng-untouched ng-valid ng-not-empty" id="fksstype2" name="1495" type="radio">
								<label for="fksstype2" class="hander"></label>
								<span class="mlmr" style="margin-right: 12px">气环境</span>
							</label>
						</div>
					</div>
					<div ng-show="fksslx=='1'" class="clear">
						<div>
							<h4 class="second_title">水环境</h4>
						</div>
						<div class="itemLine">
							<div class="twoarea">
								<span class="sixsix">应急池容量(m³)：</span>
								<span class="addipt osixsix ng-binding">-</span>
							</div>
							<div class="twoarea">
								<span class="sixsix">是否空置：</span>
								<span class="addipt osixsix ng-binding">-</span>
							</div>
						</div>
						<div class="itemLine">
							<span class="threeone">截流措施：</span>
							<span class="addipt osixsix ng-binding">是</span>
						</div>
						<div class="itemLine">
							<span class="threeone">事故废水收集措施：</span>
							<span class="addipt osixsix ng-binding">是</span>
						</div>
						<div class="itemLine">
							<span class="threeone">是否涉及清净废水：</span>
							<span class="addipt osixsix ng-binding">是</span>
						</div>
						<div class="itemLine">
							<span class="threeone">雨水排水系统风险防控措施：</span>
							<span class="addipt osixsix ng-binding">是</span>
						</div>
						<div class="itemLine">
							<span class="threeone">废水排放去向：</span>
							<span class="addipt osixsix ng-binding">是</span>
						</div>
						<div class="itemLine">
							<span class="threeone">厂区危险废物环境管理：</span>
							<span class="addipt osixsix ng-binding">是</span>
						</div>
						<div class="itemLine">
							<span class="threeone">近3年内突发水环境事件发生情况：</span>
							<span class="addipt osixsix ng-binding">是</span>
						</div>
					</div>
					<div ng-show="fksslx=='2'" class="clear ng-hide">
						<div>
							<h4 class="second_title">气环境</h4>
						</div>
						<div class="itemLine">
							<span class="threeone">是否涉及有毒有害气体：</span>
							<span class="addipt osixsix ng-binding">-</span>
						</div>
						<div class="itemLine">
							<span class="threeone">是否具备厂界有毒有害气体泄漏监控预警系统：</span>
							<span class="addipt osixsix ng-binding">-</span>
						</div>
						<div class="itemLine">
							<span class="threeone">是否符合环评及批复文件防护距离要求：</span>
							<span class="addipt osixsix ng-binding">-</span>
						</div>
						<div class="itemLine">
							<span class="threeone">近3年内突发大气环境事件发生情况：</span>
							<span class="addipt osixsix ng-binding">-</span>
						</div>
					</div>
					<!-- <table class="tables" cellspacing="0" cellpadding="0" border="1" style="width: 100%;">
						<tbody>
							<tr>
								<td>是否实现雨污分流</td>
								<td>{{fkssData.SFFYWFL}}</td>
								<td>是否设置生产废水总排放阀</td>
								<td>{{fkssData.SFSZFSZPFF}}</td>
								<td>是否设置雨水总排放阀</td>
								<td>{{fkssData.SFSZYSPFF}}</td>
							</tr>
						</tbody>
					</table> -->
				</div>
			</div>
			<div ng-show="company.sczz">
				<div class="item-content">
					<table>
						<thead>
						<tr>
							<th>序号</th>
							<th>装置名称</th>
							<th>主要原材料</th>
							<th>产品名称</th>
							<th>年产能(吨)</th>
						</tr>
						</thead>
						<tbody>
						<tr ng-repeat="item in proLists">
							<td ng-bind="$index+1"></td>
							<td ng-bind="item.name"></td>
							<td ng-bind="item.materials"></td>
							<td ng-bind="item.productName"></td>
							<!--<td ng-bind="item.prab"></td>-->
							<td>
								<span>{{item.annualCapacity}}</span>
							</td>
						</tr>
						</tbody>
					</table>
				</div>
			</div>
			<div ng-show="company.scgy" class="" style="">
				<div class="item-content">
					<table class="tables">
						<thead>
						<tr>
							<th>序号</th>
							<th>生产工艺类型</th>
							<th>生产工艺名称</th>
						</tr>
						</thead>
						<tbody>
						<tr ng-repeat="item in ylb.scgyLists">
							<td >{{storagePage.scgypageSize*(storagePage.scgypageNo-1)+$index+1}}</td>
							<td >{{item.gylx==1?'常见工艺': item.gylx==2?'其他高温或高压、涉及易燃易爆等物质的工艺过程': item.gylx==3?'具有国家规定限期淘汰的工艺名录和设备': item.gylx==4?'其他': ''}}</td>
							<td >{{item.gyName}}</td>
						</tr>
						</tbody>
					</table>
					<uib-pagination
							ng-change="getMsgList()"
							total-items="storagePage.scgytotalCount"
							ng-model="storagePage.scgypageNo"
							items-per-page="storagePage.scgypageSize"
							first-text="first"
							class="pagination"
							boundary-link-numbers="true"
							force-ellipses="true"
							direction-links="false"
							max-size= "3"
							rotate="true">
					</uib-pagination>
					<div class="clearfix"></div>
				</div>
			</div>
			<div ng-show="company.hjbhmb">
				<div class="item-content">
					<div class="seekarea" style="width: 50%;">
						<span class="fl">类型：</span>
						<div class="radiobox fl">
