package com.sucsoft.gzhbyjwz.bean.param;

import com.sucsoft.gzhbyjwz.bean.dc12.EmergencyExample;
import io.swagger.annotations.ApiModelProperty;
import org.springframework.util.StringUtils;

import java.util.Date;
import java.util.HashMap;
import java.util.Map;

public class ExampleParam extends EmergencyExample implements Param{
    @ApiModelProperty(value = "开始时间")
    private Long strTime;
    @ApiModelProperty(value = "结束时间")
    private Long endTime;

    public Long getStrTime() {
        return strTime;
    }

    public void setStrTime(Long strTime) {
        this.strTime = strTime;
    }

    public Long getEndTime() {
        return endTime;
    }

    public void setEndTime(Long endTime) {
        this.endTime = endTime;
    }

    @Override
    public Map toParam() {
        Map param = new HashMap();
        if (strTime != null){
            param.put("strTime",new Date(strTime));
        }
        if (endTime != null){
            param.put("endTime",new Date(endTime));
        }

        if (!StringUtils.isEmpty(this.getXzqyNum())){
            param.put("xzqyNum",this.getXzqyNum());
        }
        if (!StringUtils.isEmpty(this.getName())){
            param.put("name","%"+this.getName()+"%");
        }
        return param;
    }
}
