.nextPage{
    position: absolute;top: 0;height: 100%;right: 0;width: 70px;
    color: #ddd;
    text-align: center;
    opacity: 0;
    transition: all 0.2s;
}
.nextPageBtn{
    position: absolute;height: 30px;top:calc( 50% - 15px);right: 10px;
}
.nextPage i{
    font-size: 30px;
}
.nextPage:hover{
    color: #bdbdbd;
    background: -webkit-linear-gradient(right, rgba(0,0,0,1) , rgba(0,0,0,0)); /* Safari 5.1 - 6.0 */
    background: -o-linear-gradient(left, rgba(0,0,0,1), rgba(0,0,0,0)); /* Opera 11.1 - 12.0 */
    background: -moz-linear-gradient(left, rgba(0,0,0,1), rgba(0,0,0,0)); /* Firefox 3.6 - 15 */
    background: linear-gradient(to left, rgba(0,0,0,1) , rgba(0,0,0,0)); /* 标准的语法 */
    opacity: 1;
}

.prePage{
    position: absolute;top: 0;height:100%;line-height: 100%;left: 0;width: 70px;
    color: #ddd;
    text-align: center;
    opacity: 0;
    transition: all 0.2s;
}
.prePageBtn{
    position: absolute;height: 30px;top:calc( 50% - 15px);left: 10px;
}
.prePage i{
     font-size: 30px;
 }
.prePage:hover{
    color: #bdbdbd;
    background: -webkit-linear-gradient(left, rgba(0,0,0,1) , rgba(0,0,0,0)); /* Safari 5.1 - 6.0 */
    background: -o-linear-gradient(right, rgba(0,0,0,1), rgba(0,0,0,0)); /* Opera 11.1 - 12.0 */
    background: -moz-linear-gradient(right, rgba(0,0,0,1), rgba(0,0,0,0)); /* Firefox 3.6 - 15 */
    background: linear-gradient(to right, rgba(0,0,0,1) , rgba(0,0,0,0)); /* 标准的语法 */
    opacity: 1;
}
