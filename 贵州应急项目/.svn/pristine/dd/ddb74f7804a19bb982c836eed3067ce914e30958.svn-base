.popup-label {
    background-color: #FAFFBD;
    border: 1px #888 solid;
    border-radius: 4px;
    -webkit-box-shadow: 1px 2px 5px 0px rgba(0, 0, 0, 0.3);
    -moz-box-shadow:    1px 2px 5px 0px rgba(0, 0, 0, 0.3);
    box-shadow:         1px 2px 5px 0px rgba(0, 0, 0, 0.3);
  	color: #111;
  	font: 12px/20px "Helvetica Neue", Arial, Helvetica, sans-serif;
  	font-weight: bold;
  	padding: 3px 6px;
  	position: absolute;
  	white-space: nowrap;
  	top: -15px;
  	left: 20px;
    display: none;
}

.popup-label img {
    vertical-align: middle;
}

.popup-label.marker:before {
  	border-top: 6px solid transparent;
  	border-bottom: 6px solid transparent;
  	content: "";
  	border-right: 6px solid black;
  	border-right-color: inherit;
  	position: absolute;
  	left: -7px;
  	top: 5px;
}

.top{
	top: -60px;
	left: -20px;
}
.popover{
	max-width: none;
}

.angular-openlayers-map:-moz-full-screen {
    height: 100%;
}
.angular-openlayers-map:-webkit-full-screen {
    height: 100%;
}
.angular-openlayers-map:full-screen {
    height: 100%;
}

.angular-openlayers-map:not(-moz-full-screen) {
    height: 400px;
}

.angular-openlayers-map:not(-webkit-full-screen) {
    height: 400px;
}

.angular-openlayers-map:not(full-screen) {
    height: 400px;
}
.angular-openlayers-map .ol-overviewmap-box{
    border: 2px solid rgba(0,60,136,.7);
    background-color: rgba(0,60,136,.2);
}

.featureOver .popover{
	box-shadow: 5px 5px 5px rgba(0,0,0,0.35);
	border: 1px solid #ccc;
	border-radius: 0px;
    color: #4e4e4e;
    font-size: 12px;
}
.featureOver .popover>.arrow{
	display:none;
}
.featureOver .featureList{
	margin-bottom: 0px;
}
.featureOver .featureList li{
	line-height: 25px;
	cursor: pointer;
}
.featureOver .featureList li:hover{
	background-color: #cce8ff;
}
.featureOver .featureList li.active{
	background-color: #cce8ff;
	border: 1px solid #99d1ff;
}
/**
* 提示框的样式信息
*/
.tooltip {
	position: relative;
	background: white;
	border: 1px solid #005699;
    color: #333;
    padding: 4px 8px;
    white-space: nowrap;
    opacity: 1;
    filter: alpha(opacity=100);
}
 .tooltip span{
 	font-weight: bold;
  	color: #f6b436;
}
 .tooltip-distance{
	padding: 0px 2px;
	color: #666;
	border: 1px solid #666;
 }
 .tooltip-distance span{
 	color: #666;
 	font-weight: normal;
 }
 .tooltip-close{
 	border: 1px solid red;
    color: red;
    padding: 0px 2px;
    font-size: 12px;
    line-height: 12px;
 }
 .hidden {
    display: none;
}
/**
* 周边范围
*/
.nearby-control-point{
	height: 30px;
	background: white;
    margin-left: -15px;
    margin-top: -15px;
}
.nearby-control-point a{
	width: 25px;
	height: 30px;
	display: inline-block;
	background: url(../images/drag.png) no-repeat center;
    vertical-align: middle;
    cursor: pointer;
}
.nearby-control-point label{
	border: 1px solid #ddd;
	padding: 0 6px;
    height: 28px;
	line-height: 26px;
    margin-bottom: 0px;
    font-weight: normal;
}
.nearby-control-point input{
	border: 0px;
	width: 70px;
	height: 26px;
	line-height: 26px;
}